<svg xmlns="http://www.w3.org/2000/svg" width="200" height="200" viewBox="0 0 200 200">
  <rect width="200" height="200" fill="#ffffff"/>
  <g fill="#000000">
    <!-- Simplified QR code pattern -->
    <rect x="20" y="20" width="160" height="160" fill="#ffffff" stroke="#000000" stroke-width="2"/>
    <rect x="40" y="40" width="40" height="40" fill="#000000"/>
    <rect x="120" y="40" width="40" height="40" fill="#000000"/>
    <rect x="40" y="120" width="40" height="40" fill="#000000"/>
    <rect x="60" y="60" width="80" height="80" fill="#ffffff" stroke="#000000" stroke-width="2"/>
    <rect x="80" y="80" width="40" height="40" fill="#000000"/>
    <!-- Text label -->
    <text x="100" y="190" font-family="Arial" font-size="12" text-anchor="middle" fill="#000000">Scan to follow us</text>
  </g>
</svg>